{
    "name": "kitpages/mobile-app-builder-bundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony2 bundle that generates packages specific for terminals from a given html/js code base.",
    "keywords": ["symfony2", "bundle", "phonegap", "android", "iOS", "firefox OS"],
    "homepage": "https://github.com/kitpages/KitpagesMobileAppBuilderBundle",
    "license": "BSD",
    "authors": [
        {
            "name": "Philippe Le Van",
            "homepage": "https://twitter.com/plv"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "symfony/console": ">=2.1",
        "symfony/process": ">=2.1",
        "kitpages/chain-bundle": "dev-master"
    },
    "require-dev": {
        "symfony/class-loader": ">=2.1",
        "symfony/yaml": ">=2.1",
        "symfony/finder": ">=2.1",
        "symfony/browser-kit": ">=2.1"
    },
    "autoload": {
        "psr-0": { "Kitpages\\MobileAppBuilderBundle": "" }
    },
    "target-dir": "Kitpages/MobileAppBuilderBundle"
}